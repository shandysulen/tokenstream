{
  "name": "@tokenstream/workspace",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:db": "pnpm --filter @tokenstream/db build",
    "build:components": "pnpm --filter @tokenstream/components build",
    "clean:modules": "rimraf node_modules",
    "clean": "pnpm clean:modules",
    "clean:components": "pnpm clean:components:dist",
    "clean:components:dist": "rimraf packages/components/dist",
    "clean:web:next": "pnpm --filter @tokenstream/web clean:next",
    "dev:web": "pnpm --filter @tokenstream/web dev",
    "start:indexer": "pnpm --filter @tokenstream/indexer start"
  }
}
